{
    "name": "technikermathe/blade-lucide-icons",
    "description": "A package to easily make use of Lucide icons in your Laravel Blade views.",
    "license": "MIT",
    "keywords": ["Blade", "Heroicons", "Laravel", "LucideIcons"],
    "authors": [
        {
            "name": "Dan Pock",
            "role": "Original Developer"
        },
        {
            "name": "Pascale Beier",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/technikermathe/blade-lucide-icons",
    "require": {
        "php": "~8.1.0|~8.2.0|~8.3.0",
        "blade-ui-kit/blade-icons": "^1.4",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0",
        "phpunit/phpunit": "^9.0|| ^10.0",
        "spatie/phpunit-snapshot-assertions": "^4.2.14 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Technikermathe\\LucideIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Technikermathe\\LucideIcons\\BladeLucideIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
